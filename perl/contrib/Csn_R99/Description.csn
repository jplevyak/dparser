---------------------------------------------------------------------------------
--
--   CSN-DEFINITIONS R99 ( qcswilk 030212 )
--   based on 0460-8g0.doc
--
--------------------------------------------------------------------------------
-- The description is based on extended CSN.1 
--------------------------------------------------------------------------------

< padding bits > ::= { 0 < spare padding > | null };

< Downlink RLC/MAC control message MT > ::= -MESSAGE
  < MESSAGE_TYPE : bit (6) == 100001 > < Packet Access Reject message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 000001 > < Packet Cell Change Order message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 000010 > < Packet Downlink Assignment message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 000011 > < Packet Measurement Order message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 100010 > < Packet Paging Request message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 100011 > < Packet PDCH Release message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 000100 > < Packet Polling Request message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 000101 > < Packet Power Control/Timing Advance message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 100100 > < Packet PRACH Parameters message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 000110 > < Packet Queueing Notification message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 000111 > < Packet Timeslot Reconfigure message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 001000 > < Packet TBF Release message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 001001 > < Packet Uplink Ack/Nack message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 001010 > < Packet Uplink Assignment message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 100101 > < Packet Downlink Dummy Control Block message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 110001 > < PSI1 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 110010 > < PSI2 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 110011 > < PSI3 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 110100 > < PSI3 bis message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 110101 > < PSI4 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 110110 > < PSI5 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 110000 > < PSI6 message content > M V b:178-? |
  -- < MESSAGE_TYPE : bit (6) == 111000 > < PSI7 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 111001 >  < PSI8 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 110111 >  < PSI13 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 111010 >  < PSI14 message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 111100 >  < PSI3 ter message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 111101 >  < PSI3 quater message content > M V b:178-? |
  < MESSAGE_TYPE : bit (6) == 111110 >  < PSI15 message content > M V b:178-?
   ! < Unknown message type : bit (6)>
     < Default downlink message content >  ;

< Default downlink message MT > ::=  -- -MESSAGE
  < MESSAGE_TYPE_UNKNOWN : bit (6) >
  < Default downlink message contentent > ;

< Default downlink message contentent > ::=
  < PAGE_MODE >
    bit ** --
;

< Uplink RLC/MAC control message MO > ::= -MESSAGE
  < MESSAGE_TYPE : bit (6) == 000000 >  < Packet Cell Change Failure message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 000001 >  < Packet Control Acknowledgement message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 000010 >  < Packet Downlink Ack/Nack message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 000011 >  < Packet Uplink Dummy Control Block message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 000100 >  < Packet Measurement Report message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 001010 >  < Packet Enhanced Measurement Report message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 000101 >  < Packet Resource Request message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 000110 >  < Packet Mobile TBF Status message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 000111 >  < Packet PSI Status message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 001000 >  < EGPRS Packet Downlink Ack/Nack message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 001001 >  < Packet Pause message content > M V b:178
	|
  < MESSAGE_TYPE : bit (6) == 001011 >  < Additional MS Radio Access Capabilities message content> M V b:178
	;

< Packet Access Reject message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 100001 >
< Packet Access Reject message content > M V b:178-?
;

< Packet Access Reject message content > ::=
  < PAGE_MODE >
  < Reject struct >
  { 0 | 1 < Reject struct > } ** 0
   < padding bits >     -- truncation at end of message allowed, bits '0' assumed
   ! < Distribution part error : bit (*)  > ;

< Reject struct > ::=
  {  0  < TLLI : bit (32) >
   | 1  { 0 < Packet Request Reference IE >
        | 1 < Global TFI IE > } }
  { 0 | 1 < WAIT_INDICATION : bit (8) >
        < WAIT _INDICATION_SIZE : bit (1) > }
   ! < Ignore : bit (*)  > ;

< Packet Control Acknowledgement message MO > ::=  -MESSAGE   -- RLC/MAC control block format
< MESSAGE_TYPE : bit(6) == 000001 >
< Packet Control Acknowledgement message content >  M V b:178
; 

< Packet Control Acknowledgement message content > ::= 
  < TLLI : bit (32) >
  < CTRL_ACK : bit (2) >
  < padding bits > ;

< Packet Control Acknowledgement 11 bit message > ::=     -- 11-bit access burst format
  < MESSAGE_TYPE : bit (9) == 1111 1100 1 >
  < CTRL_ACK : bit (2) > ;

< Packet Control Acknowledgement 8 bit message > ::=      -- 8-bit access burst format
  < MESSAGE_TYPE : bit (6) == 0111 11 >
  < CTRL_ACK : bit (2) > ;

< Packet Cell Change Failure message MO > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000000 >
< Packet Cell Change Failure message content > M V b:178
; 

< Packet Cell Change Failure message content > ::=  
  < TLLI : bit (32) >
  < ARFCN : bit (10) >
  < BSIC : bit (6) >
  < CAUSE : 
          < Frequency not implemented:    0000> |
          < No response on target cell:   0001> |
          < Immediate Assign Reject or Packet Access Reject on target cell: 0010> |
          < On going CS connection:       0011> |
          < Anonymous Access:             0100> |
          < MS in GMM Standby state:      0101> |
          < Forced to the Standby state:  0110> 
  >
  {  
       null 
     | < Receiver compatible with earlier release : 0 > 
     | < Additions in R99 : 1 > 
      { 0 | 1 < FDD-ARFCN : bit (14) >            -- 3G UTRAN FDD
          < Diversity : bit >
          { 0 | 1 < Bandwidth_FDD : bit (3) > }
          < SCRAMBLING_CODE : bit (9) > }
      { 0 | 1 < TDD-ARFCN : bit (14) >            -- 3G UTRAN TDD
          < Diversity : bit >
          { 0 | 1 < Bandwidth_TDD : bit (3) > }
          < Cell Parameter : bit (7) >
          < Sync Case : bit > }
-- ORG:      < padding bits > } ;
           } < padding bits > ;

< Packet Cell Change Order message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000001 >
< Packet Cell Change Order message content >  M V b:178-? 
; 


< Packet Cell Change Order message content > ::=  
  < PAGE_MODE >
  { { 0 < Global TFI IE  >
     | 10 < TLLI : bit (32) > }
    { 0   
      { < IMMEDIATE_REL : bit >
        < GSM target cell struct >
         ! < Non-distribution part error : bit (*)  > }
       | 1
      { 00  -- Message escape
        { < IMMEDIATE_REL : bit >
          < 3G-target cell struct >
         ! < Non-distribution part error : bit (*)  > }
       ! < Message escape : { 01 | 10 | 11} bit (*)  > } }
     ! < Address information part error : bit (*)  > }
   < spare bits > -- qcswilk 030415  
   ! < Distribution part error : bit (*)  > ;

< GSM target cell struct > ::=
  < ARFCN : bit (10) >
  < BSIC : bit (6) >
  < NC Measurement Parameters struct > 
  { 
     null 
   | < Receiver compatible with earlier release : 0 > 
    { 0 | 1 < LSA Parameters IE > }
    {  
       null 
     | < Receiver compatible with earlier release : 0 > 
     | < Additions in R99 : 1 > 
      < ENH Measurement parameters struct > 
      < padding bits > } } ;

< 3G-target cell struct > ::=
  { 0 | 1 < FDD-ARFCN : bit (14) >            -- 3G UTRAN FDD
      < Diversity : bit >
      { 0 | 1 < Bandwidth_FDD : bit (3) > }
      < SCRAMBLING_CODE : bit (9) > }
  { 0 | 1 < TDD-ARFCN : bit (14) >            -- 3G UTRAN TDD
      < Diversity : bit >
      { 0 | 1 < Bandwidth_TDD : bit (3) > }
      < Cell Parameter : bit (7) >
      < Sync Case : bit > } 
  < padding bits > ;

< NC Measurement Parameters struct > ::=
  < NETWORK_CONTROL_ORDER >
  { 0 | 1 < NC_NON_DRX_PERIOD : 
                < No non-DRX mode after a measurement report has been sent  : 000 > |
                < 0,24 sec : 001 > |
                < 0,48 sec - default value : 010 > |
                < 0,72 sec : 011 > |
                < 0,96 sec : 100 > |
                < 1,20 sec : 101 > |
                < 1,44 sec : 110 > |
                < 1,92 sec : 111 > 
           >
    < NC_REPORTING_PERIOD_I : 
                < 0,48 sec : 000 > |
                < 0,96 sec : 001 > |
                < 1,92 sec : 010 > |
                < 3,84 sec - default value for NC_REPORTING_PERIOD_T : 011 > |
                < 7,68 sec : 100 > |
                < 15,36 sec : 101 > |
                < 30,72 sec : 110 > |
                < 61,44 sec - default value for NC_REPORTING_PERIOD_I : 111 > 
     >
    < NC_REPORTING_PERIOD_T : 
                < 0,48 sec : 000 > |
                < 0,96 sec : 001 > |
                < 1,92 sec : 010 > |
                < 3,84 sec - default value for NC_REPORTING_PERIOD_T : 011 > |
                < 7,68 sec : 100 > |
                < 15,36 sec : 101 > |
                < 30,72 sec : 110 > |
                < 61,44 sec - default value for NC_REPORTING_PERIOD_I : 111 > 
     > }
  { 0 | 1 <  NC Frequency list struct > } ;

< NC Frequency list struct > ::=
  { 0 | 1 { < NR_OF_REMOVED_FREQ : bit (5) >
      { < REMOVED_FREQ_INDEX : bit (6) > } * (1 + val(NR_OF_REMOVED_FREQ)) } }
  { 0 | 1 < Add Frequency list struct >} ** 0;

< Add Frequency list struct > ::=
  < START_FREQUENCY : bit (10) >
  < BSIC : bit (6) >
  { 0 | 1 < Cell Selection struct PMO > }
  < NR_OF_FREQUENCIES : bit (5) >
  < FREQ_DIFF_LENGTH : bit (3) >
  { < FREQUENCY_DIFF : bit (1+val(FREQ_DIFF_LENGTH)) >
    < BSIC : bit (6) >
    { 0 | 1 < Cell Selection struct PMO > } } * (val(NR_OF_FREQUENCIES));

< Cell Selection struct PMO > ::=
  < CELL_BAR_ACCESS_2 : bit (1) >
  < EXC_ACC : bit >
  < SAME_RA_AS_SERVING_CELL : bit (1) >
  { 0 | 1 < GPRS_RXLEV_ACCESS_MIN : bit (6) >
    < GPRS_MS_TXPWR_MAX_CCH : bit (5) > }
  { 0 | 1 < GPRS_TEMPORARY_OFFSET : bit (3) >
    < GPRS_PENALTY_TIME : bit (5) > }
  { 0 | 1 < GPRS_RESELECT_OFFSET : bit (5) > }
  { 0 | 1 < HCS struct > }
  { 0 | 1 < SI13_PBCCH_LOCATION struct > } ;

< SI13_PBCCH_LOCATION struct > ::=
  {  0 < SI13_LOCATION  : bit (1) >
   | 1 < PBCCH_LOCATION : bit (2) >
    < PSI1_REPEAT_PERIOD : bit (4) > } ;

< HCS struct > ::=
  < PRIORITY_CLASS : bit (3) >
  < HCS_THR : bit (5) > ;

< ENH Measurement parameters struct > ::= 
  { 0 < BA_IND : bit > < 3G_BA_IND : bit > | 1 < PSI3_CHANGE_MARK : bit(2) > }
  < PMO_IND : bit >
  < REPORT_TYPE : bit >
  < REPORTING_RATE : bit >
  < INVALID_BSIC_REPORTING : bit >
  { 0 | 1 < 3G Neighbour Cell Description struct > }
  { 0 | 1 < GPRS REP PRIORITY Description struct > }
  { 0 | 1 < GPRS MEASUREMENT PARAMETERS Description struct > }
  { 0 | 1 < GPRS 3G MEASUREMENT PARAMETERS Description struct > } ;

< 3G Neighbour Cell Description struct > ::=
  { 0 | 1 < Index_Start_3G : bit (7)> }
  { 0 | 1 < Absolute_Index_Start_EMR : bit (7)> }
  { 0 | 1 < UTRAN FDD Description struct > }
  { 0 | 1 < UTRAN TDD Description struct >}
--  { 0 | 1 < CDMA2000 Description struct > }
  { 0 | 1 <  REMOVED_3GCELL_Description struct > } ;

< REMOVED_3GCELL_Description struct > ::=
  < N1 : bit (2) >
  { < N2 : bit (5) >
    { < REMOVED_3GCELL_INDEX : bit (7) >
      < 3G_CELL_DIFF_LENGTH : bit (3) >
      < 3GCELL_DIFF : bit (val(3G_CELL_DIFF_LENGTH)) >
    } * (1+val(N2))
  } * (1+val(N1)) ;

< UTRAN FDD Description struct> ::=
  { 0 | 1 < Bandwidth_FDD : bit (3) > }
  { 0 | 1 < Repeated UTRAN FDD Neighbour Cells struct > } ** 0 ;

< Repeated UTRAN FDD Neighbour Cells struct > ::=
  0 < FDD-ARFCN : bit (14) >    -- The value '1' was used in an earlier
                                -- version of the protocol and shall not be used.
  < FDD_Indic0 : bit >
  < NR_OF_FDD_CELLS : bit (5) >
  < FDD_CELL_INFORMATION Field : bit(Table_FDD_CELL_INFORMATION(NR_OF_FDD_CELLS)) > ; -- p(x) defined in table 11.2.9b.2.a/3GPP TS 04.60

< UTRAN TDD Description struct > ::=
  { 0 | 1 < Bandwidth_TDD : bit (3) > }
  { 0 | 1 < Repeated UTRAN TDD Neighbour Cells struct PCCO > } ** 0 ;

< Repeated UTRAN TDD Neighbour Cells struct PCCO > ::=
  0 < TDD-ARFCN : bit (14) >    -- The value ‘1’ was used in an earlier
                                -- version of the protocol and shall not be used.
  < TDD_Indic0 : bit >
  < NR_OF_TDD_CELLS : bit (5) >
  < TDD_CELL_INFORMATION Field : bit(Table_TDD_CELL_INFORMATION(NR_OF_TDD_CELLS)) > ; -- q(x) defined in table 11.2.9b.2.b/3GPP TS 04.60.


< GPRS REP PRIORITY Description struct> ::=
  < Number_Cells : bit(7) >
  { < REP_PRIORITY : bit >} * (val(Number_Cells)) ;

< GPRS MEASUREMENT PARAMETERS Description struct > ::=
  { 0 | 1 < MULTIBAND_REPORTING : bit (2) > }
  { 0 | 1 < SERVING_BAND_REPORTING : bit (2) > }
  < SCALE_ORD : bit(2) >

  { 0 | 1 < 900_REPORTING_OFFSET : bit (3) > 
      < 900_REPORTING_THRESHOLD : bit (3) > }

  { 0 | 1 < 1800_REPORTING_OFFSET : bit (3) > 
      < 1800_REPORTING_THRESHOLD : bit (3) > }

  { 0 | 1 < 400_REPORTING_OFFSET : bit (3) > 
      < 400_REPORTING_THRESHOLD : bit (3) > }

  { 0 | 1 < 1900_REPORTING_OFFSET : bit (3) > 
      < 1900_REPORTING_THRESHOLD : bit (3) > }

  { 0 | 1 < 850_REPORTING_OFFSET : bit (3) > 
      < 850_REPORTING_THRESHOLD : bit (3) > } ;


< GPRS 3G MEASUREMENT PARAMETERS Description struct > ::=
  < Qsearch_P : bit (4) > 
  < 3G_SEARCH_PRIO : bit >
  { 0 | 1 < FDD_REP_QUANT : bit >                         -- FDD Parameters
      < FDD_MULTIRAT_REPORTING : bit (2) > }
   
  { 0 | 1 < FDD_REPORTING_OFFSET : bit (3) > 
      < FDD_REPORTING_THRESHOLD : bit (3) > }

  { 0 | 1 < TDD_MULTIRAT_REPORTING : bit (2) > }                -- TDD Parameters
  { 0 | 1 < TDD_REPORTING_OFFSET : bit (3) > 
      < TDD_REPORTING_THRESHOLD : bit (3) > }

  { 0 | 1 < CDMA2000_MULTIRAT_REPORTING : bit (2) > }           -- CDMA2000 Parameters
  { 0 | 1 < CDMA2000_REPORTING_OFFSET : bit (3) > 
      < CDMA2000_REPORTING_THRESHOLD : bit (3) > } ;

< Packet channel request 11 bit message > ::=  -- -MESSAGE
--< MESSAGE_TYPE : bit(6) == xxxxxx >
< Packet channel request 11 bit message content > 
-- < EGPRS Packet channel request message content >
;  


< Packet channel request 11 bit message content > ::=  
  < One Phase Access Request :                  0       < MultislotClass : bit (5) >
                                                        < Priority :       
                                                            < Radio Priority 1 - Highest priority :  00 > |
                                                            < Radio Priority 2:                      01 > |
                                                            < Radio Priority 3:                      10 > |
                                                            < Radio Priority 4 - Lower priority :    11 >
                                                         >
                                                        < RandomBits : bit (3) > >
  | < Short Access Request :                    100     < NumberOfBlocks : bit (3) >
                                                        < Priority :       
                                                            < Radio Priority 1 - Highest priority :  00 > |
                                                            < Radio Priority 2:                      01 > |
                                                            < Radio Priority 3:                      10 > |
                                                            < Radio Priority 4 - Lower priority :    11 >
                                                         >
                                                        < RandomBits : bit (3) > >
  | < Two Phase Access Request :                110000  
                                                        < Priority :       
                                                            < Radio Priority 1 - Highest priority :  00 > |
                                                            < Radio Priority 2:                      01 > |
                                                            < Radio Priority 3:                      10 > |
                                                            < Radio Priority 4 - Lower priority :    11 >
                                                         >
                                                        < RandomBits : bit (3) > >
  | < Page Response :                           110001  < RandomBits : bit (5) > >
  | < Cell Update :                             110010  < RandomBits : bit (5) > >
  | < MM Procedure :                            110011  < RandomBits : bit (5) > >
  | < Single Block Without TBF Establishment :  110100  < RandomBits : bit (5) > > ;

  
< Packet channel request 8 bit message > ::=  -- -MESSAGE
-- TBC < MESSAGE_TYPE : bit(6) == xxxxxx >
< Packet channel request 8 bit message content> 
;  
< Packet channel request 8 bit message content> ::=  
  < One Phase Access Request :                  1       < MultislotClass : bit (5) >
                                                        < RandomBits :     bit (2) >>
  | < Short Access Request :                    00      < NumberOfBlocks : bit (3) >
                                                        < RandomBits : bit (3) >>
  | < Two Phase Access Request :                01000 < RandomBits : bit (3) >>
  | < Page Response :                           01001 < RandomBits : bit (3) >>
  | < Cell Update :                             01010 < RandomBits : bit (3) >>
  | < MM Procedure :                            01011 < RandomBits : bit (3) >>
  | < Single Block Without TBF Establishment :  01100 < RandomBits : bit (3) >> ;


< Packet Downlink Ack/Nack message MO > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000010 >
< Packet Downlink Ack/Nack message content >  M V b:178
; 

< Packet Downlink Ack/Nack message content > ::=  
  < DOWNLINK_TFI : bit (5) >
  < Ack/Nack Description IE >
  { 0 | 1 < Channel Request Description IE > }
  < Channel Quality Report struct >
  { 
       null 
     | < Receiver compatible with earlier release : 0 > 
     | < Additions in R99 : 1 > 
    { 0 | 1 < PFI : bit(7) > }
-- ORG:  < padding bits > };
       } < padding bits >;

< Channel Quality Report struct > ::=
  < C_VALUE :  bit (6) >
  < RXQUAL :   bit (3) >
  < SIGN_VAR : bit (6) >
  { 0 | 1 < I_LEVEL_TN0 : bit (4) > }
  { 0 | 1 < I_LEVEL_TN1 : bit (4) > }
  { 0 | 1 < I_LEVEL_TN2 : bit (4) > }
  { 0 | 1 < I_LEVEL_TN3 : bit (4) > }
  { 0 | 1 < I_LEVEL_TN4 : bit (4) > }
  { 0 | 1 < I_LEVEL_TN5 : bit (4) > }
  { 0 | 1 < I_LEVEL_TN6 : bit (4) > }
  { 0 | 1 < I_LEVEL_TN7 : bit (4) > } ;

-- 11.2.6a	EGPRS Packet Downlink Ack/Nack --
< EGPRS Packet Downlink Ack/Nack message MO > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 001000 >
< EGPRS Packet Downlink Ack/Nack message content > M V b:178
; 


< EGPRS Packet Downlink Ack/Nack message content > ::=  
    < DOWNLINK_TFI : bit (5) >
    < MS OUT OF MEMORY : bit(1)>
    { 0 | 1 < EGPRS Channel Quality Report IE >}
    { 0 | 1 < Channel Request Description IE >}
    { 0 | 1 < PFI : bit(7) > }
    { 0 | 1 < Extension Bits : Extension Bits IE > }        -- clause 12.26
    < EGPRS Ack/Nack Description IE >
  <padding bits > ;

< Packet Downlink Assignment message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000010 >
< Packet Downlink Assignment message content >  M V b:178-?
; 


< Packet Downlink Assignment message content > ::=  
  < PAGE_MODE >
  { 0 | 1 <PERSISTENCE_LEVEL : bit (4) > * 4 }
  { { 0 < Global TFI IE >
     | 10 < TLLI : bit (32) > }
    { 0   -- Message escape
      { < MAC_MODE : bit (2) >
        < RLC_MODE : bit (1) >
        < CONTROL_ACK : bit (1) >
        < TIMESLOT_ALLOCATION : bit (8) >
        < Packet Timing Advance IE >
        { 0 | 1 < P0 : bit (4) >
          < BTS_PWR_CTRL_MODE : bit (1) > 
          < PR_MODE :bit (1) > }
        { { 0 | 1 < Frequency Parameters IE > }
          { 0 | 1 < DOWNLINK_TFI_ASSIGNMENT : bit (5) > }
          { 0 | 1 < Power Control Parameters IE > }
          { 0 | 1 < Starting Frame Number Description IE > }
          { 0 | 1 < Measurement Mapping struct > }
          { 
           null 
         | < Receiver compatible with earlier release : 0 > 
         | < Additions in R99 : 1 > 
          { 0 | 1 < EGPRS Window Size IE >
              < LINK_QUALITY_MEASUREMENT_MODE : bit (2) >
              { 0 | 1 < BEP_PERIOD2 : bit(4) > }}
          { 0 | 1 <Packet Extended Timing Advance : bit (2)> }

          { 0 | 1 < COMPACT reduced MA IE > }
        < padding bits > }} //          -- truncation at end of message allowed, bits '0' assumed
         ! < Non-distribution part error : bit (*)  > }
       ! < Message escape : 1 bit (*)  > }
     ! < Address information part error : bit (*)  > }
   < spare bits > -- qcswilk 030415   
   ! < Distribution part error : bit (*)  > ;

< Measurement Mapping struct > ::=
  < Starting Frame Number Description IE >
  < MEASUREMENT_INTERVAL : bit (5) >
  < MEASUREMENT_BITMAP : bit (8) > ;

< Packet Downlink Dummy Control Block message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 100101 >
< Packet Downlink Dummy Control Block message content >  M V b:178-?
; 


< Packet Downlink Dummy Control Block message content > ::=  
  < PAGE_MODE >
  { 0 | 1 <PERSISTENCE_LEVEL : bit (4) > * 4 }
  < padding bits >
   ! < Distribution part error : bit (*)  > ;

< Packet Uplink Dummy Control Block message MO > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000011 >
< Packet Uplink Dummy Control Block message content > M V b:178

; 


< Packet Uplink Dummy Control Block message content > ::=  
  < TLLI : bit (32) >
  < padding bits > ;

< Packet Measurement Report message MO > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000100 >
< Packet Measurement Report message content > M V b:178
   
  ; 

< Packet Measurement Report message content > ::=  
  < TLLI : bit (32) >
  { 0 | 1 < PSI5_CHANGE_MARK : bit (2) > }
  { { 0 < NC Measurement Report :  < NC Measurement Report struct 1 > > }
      |
    { 1 < EXT Measurement Report struct >
      { 
         null 
       | < Receiver compatible with earlier release : 0 > 
       | < Additions in R99 : 1 > 
          { 0 | 1   
                {  0 < BA_USED : bit > < 3G_BA_USED : bit > 
                 | 1 < PSI3_CHANGE_MARK : bit(2) > 
                }     
                < PMO_USED : bit > 
          }
          { 0 | 1 < 3G Measurement Report struct > }  
      }
    } 
   }
  < padding bits > 
;

< NC Measurement Report struct 1 > ::=
  < NC_MODE : bit (1) >
  < RXLEV_SERVING_CELL : bit (6) >
  { 0 | 1 < INTERFERENCE_SERVING_CELL : bit (6) > }
  < NUMBER_OF_NC_MEASUREMENTS : bit (3) >
  { < FREQUENCY_N : bit (6) >
    { 0 | 1 < BSIC_N : bit (6) > }
    < RXLEV_N : bit (6) >   } * (val(NUMBER_OF_NC_MEASUREMENTS)) ;

< EXT Measurement Report struct > ::=
  < EXT_REPORTING_TYPE: 00 | 01 | 10 >
  { 0 | 1 
    { 0 | 1 < I_LEVEL_TN0 : bit (6) > }
    { 0 | 1 < I_LEVEL_TN1 : bit (6) > }
    { 0 | 1 < I_LEVEL_TN2 : bit (6) > }
    { 0 | 1 < I_LEVEL_TN3 : bit (6) > }
    { 0 | 1 < I_LEVEL_TN4 : bit (6) > }
    { 0 | 1 < I_LEVEL_TN5 : bit (6) > }
    { 0 | 1 < I_LEVEL_TN6 : bit (6) > }
    { 0 | 1 < I_LEVEL_TN7 : bit (6) > } }
  < NUMBER_OF_MEASUREMENTS : bit (5) >
  { < FREQUENCY_N : bit (6) >
    { 0 | 1 < BSIC_N : bit (6) > }
    < RXLEV_N : bit (6) > } * (val(NUMBER_OF_MEASUREMENTS)) ;

< 3G Measurement Report struct > ::=
  < N_3G: bit (3) >
  { < 3G_CELL_LIST_INDEX : bit (7) >
    < REPORTING_QUANTITY : bit (6) > } * (val(N_3G) + 1) ;


< Packet Measurement Order message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000011 >
< Packet Measurement Order message content >  M V b:178-?
   
  ; 


< Packet Measurement Order message content > ::=  
  < PAGE_MODE >
  { { 0 < Global TFI IE > 
     | 10 < TLLI : bit (32) > }
    { < PMO_INDEX : bit (3) >
      < PMO_COUNT : bit (3) >
      { 0 | 1 < NC Measurement Parameters struct >  }
      { 0 | 1 < EXT Measurement Parameters struct >  }
      {   
         null 
       | < Receiver compatible with earlier release : 0 > 
       | < Additions in R98 : 1 > 
          { 0 | 1 < LSA Parameters IE > }
          { 
             null 
           | < Receiver compatible with earlier release : 0 > 
           | < Additions in R99 : 1 > 
             { 0 | 1 < ENH Measurement Parameters struct > }
             < padding bits > } }
       ! < Non-distribution part error : bit (*)  > }
     ! < Address information part error : bit (*)  > }
   < spare bits > -- qcswilk 030415   
   ! < Distribution part error : bit (*)  > ;

< EXT Measurement Parameters struct > ::=
  {   < EXT_MEASUREMENT_ORDER : bit (2) == 00 > 
  | < EXT_MEASUREMENT_ORDER : bit (2) == 01 >   < EM1 struct > 
  | < EXT_MEASUREMENT_ORDER : bit (2) == 10 > 
  | < EXT_MEASUREMENT_ORDER : bit (2) == 11 > } ;

< EM1 struct > ::=
  { 0 | 1 { < EXT_REPORTING_TYPE: bit (2) == 00 > 
  |   < EXT_REPORTING_TYPE: bit (2) == 01 >   < NCC_PERMITTED : bit (8) > 
  |   < EXT_REPORTING_TYPE: bit (2) == 10 >   { 0 | 1 < INT_FREQUENCY : bit (5) > } 
      | < EXT_REPORTING_TYPE: bit (2) == 11 > } }

  { 0 | 1 < EXT_REPORTING_PERIOD : 
                              < 60 sec  : 000 > |
                              < 120 sec : 001 > |
                              < 240 sec : 010 > |
                              < 480 sec : 011 > |
                              < 960 sec : 100 > |
                              < 1920 sec - default value : 101 > |
                              < 3840 sec : 110 > |
                              < 7680 sec : 111 > 
         >} 
  { < EXT Frequency List Description struct >  } ;

< EXT Frequency List Description struct > ::=
  < EXT Frequency List struct > { 0 | 1 < EXT Frequency List struct > } ** 0 ;

< EXT Frequency List struct > ::=
  < START_FREQUENCY : bit (10) >
  < NR_OF_FREQUENCIES : bit (5) >
  < FREQ_DIFF_LENGTH : bit (3) >
  { < FREQUENCY_DIFF : bit (1+val(FREQ_DIFF_LENGTH)) > } * (val(NR_OF_FREQUENCIES)) ;



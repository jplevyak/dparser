< Packet Uplink Ack/Nack message content >  M V b:178-?
   
  ; 


< Packet Uplink Ack/Nack message content > ::=  
  < PAGE_MODE >
  { 00 
	  < UPLINK_TFI : bit (5) >
    { 0   -- Message escape
      { < CHANNEL_CODING_COMMAND : 
                                  < CS-1: 00 > |
                                  < CS-2: 01 > |
                                  < CS-3: 10 > |
                                  < CS-4: 11 >
        >
        < Ack/Nack Description IE > 
        { 0 | 1 < CONTENTION_RESOLUTION_TLLI : bit (32) > }
        { 0 | 1 < Packet Timing Advance IE >  }
        { 0 | 1 < Power Control Parameters IE >  }
        { 0 | 1 < Extension Bits IE > }        -- clause 12.26
        { 0 | 1 < Fixed Allocation Parameters : < Fixed Allocation struct PUAN > > }
        { 
           null 
         | < Receiver compatible with earlier release : 0 > 
         | < Additions in R99 : 1 > 
            { 0 | 1 <Packet Extended Timing Advance : bit (2)>}
            <TBF_EST : bit (1)>
            < padding bits > }
         !  < Non-distribution part error : bit (*)  > 
				}
    | 1                     -- Message escape bit used to define EGPRS message contents  
        { 00 { < EGPRS Modulation and Coding IE >
               < RESEGMENT : bit (1) >
               < PRE_EMPTIVE_TRANSMISSION : bit (1) >
               < PRR RETRANSMISSION REQUEST : bit (1) >
               < ARAC RETRANSMISSION REQUEST : bit (1) >
               { 0 | 1 < CONTENTION_RESOLUTION_TLLI : bit (32) > }
               <TBF_EST : bit (1)>
				 { 0 | 1 < Packet Timing Advance IE >  }
				 { 0 | 1 < Packet Extended Timing Advance : bit (2)> }
				 { 0 | 1 < Power Control Parameters IE >  }
				 { 0 | 1 < Extension Bits IE > }        -- clause 12.26
				 < EGPRS Ack/Nack Description IE > 
				 { 0 | 1 < Fixed Allocation Parameters : < Fixed Allocation struct PUAN > > } 
              < padding bits >
              ! < Non-distribution part error : bit (*)  > 
			       }
             ! < Message escape : {01| 10 | 11} bit (*)  >
	       } -- Extended for future changes
     }
		 ! < Address information part error : bit (*)  > 
	 }
	 < spare bits > -- qcswilk 030415
   ! < Distribution part error : bit (*)  > 
;

< Fixed Allocation struct PUAN > ::=
  < FINAL_ALLOCATION : bit (1) >
  { 0   -- Repeat Allocation
    < TS_OVERRIDE : bit (8) >
  | 1   -- Allocation with Allocation bitmap
    < TBF Starting Time : < Starting Frame Number Description IE > >
    { 0 | 1 <TIMESLOT_ALLOCATION : bit (8) > }
    { 0 { 0     -- with length of Allocation Bitmap
        < BLOCKS_OR_BLOCK_PERIODS : bit (1) >
        < ALLOCATION_BITMAP_LENGTH : bit (7) >
        < ALLOCATION_BITMAP : bit (val(ALLOCATION_BITMAP_LENGTH)) >
      | 1     -- without length of Allocation Bitmap (fills remainder of the message)
        < ALLOCATION_BITMAP : bit ** > } 
       ! < Message escape : 1 bit (*)  > }
  };


  < TLLI : bit (32) >
  < RAI : bit (48) >
  < padding bits > ;


< Packet Timeslot Reconfigure message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000111 >
< Packet Timeslot Reconfigure message content >  M V b:178-?
   
  ; 


< Packet Timeslot Reconfigure message content > ::=  
  < PAGE_MODE >
  { 0 < Global TFI IE > 
    { 0   -- Message escape
      { < CHANNEL_CODING_COMMAND : 
                                  < CS-1: 00 > |
                                  < CS-2: 01 > |
                                  < CS-3: 10 > |
                                  < CS-4: 11 >
         >
        < Global Packet Timing Advance IE > 
        < DOWNLINK_RLC_MODE : bit (1) >
        < CONTROL_ACK : bit (1) >
        { 0 | 1 < DOWNLINK_TFI_ASSIGNMENT : bit (5) > }
        { 0 | 1 < UPLINK_TFI_ASSIGNMENT : bit (5) > }
        < DOWNLINK_TIMESLOT_ALLOCATION : bit (8) >
        { 0 | 1 < Frequency Parameters IE >  }
        { 0 < Dynamic Allocation struct PTR > 
        | 1 < Fixed Allocation struct PTR > }
        {    
           null 
         | < Receiver compatible with earlier release : 0 > 
         | < Additions in R99 : 1 > 
           { 0 | 1 <Packet Extended Timing Advance : bit (2)> }
           < padding bits > }
         ! < Non-distribution part error : bit (*)  > }
    | 1       -- Message escape bit used to define EGPRS message contents  -MESSAGE
      { 00 {  { 0 | 1 < COMPACT reduced MA IE > }
          < EGPRS Channel Coding Command : < EGPRS Modulation and Coding IE >>
          < RESEGMENT : bit (1) >
          { 0 | 1 < DOWNLINK EGPRS Window Size : < EGPRS Window Size IE >>}
          { 0 | 1 < UPLINK EGPRS Window Size : < EGPRS Window Size IE >>}
          < LINK_QUALITY_MEASUREMENT_MODE : bit (2) >
        < Global Packet Timing Advance IE >
          { 0 | 1 <Packet Extended Timing Advance : bit (2)> }
        < DOWNLINK_RLC_MODE : bit (1) >
        < CONTROL_ACK : bit (1) >
        { 0 | 1 < DOWNLINK_TFI_ASSIGNMENT : bit (5) > }
        { 0 | 1 < UPLINK_TFI_ASSIGNMENT : bit (5) > }
        < DOWNLINK_TIMESLOT_ALLOCATION : bit (8) >
        { 0 | 1 < Frequency Parameters IE >  }
           { 0  < Dynamic Allocation struct PTR >
           | 1  < Fixed Allocation struct PTR > }
        < padding bits >
         ! < Non-distribution part error : bit (*)  > }
       ! < Message escape : { 01| 10 | 11} bit (*)  > }} -- Extended for future changes
     ! < Address information part error : bit (*)  > }
   < spare bits > -- qcswilk 030415 
   ! < Distribution part error : bit (*)  > ;

<Dynamic Allocation struct PTR > ::=
  < EXTENDED_DYNAMIC_ALLOCATION : bit (1) >
  { 0 | 1 < P0 : bit (4) > 
      <PR_MODE : bit (1)>}
  < USF_GRANULARITY : bit (1) >
  { 0 | 1 < RLC_DATA_BLOCKS_GRANTED : bit (8) > }
  { 0 | 1 < TBF Starting Time : < Starting Frame Number Description IE > > }
  { 0           -- Timeslot Allocation
    { 0 | 1 < USF_TN0 : bit (3) > }
    { 0 | 1 < USF_TN1 : bit (3) > }
    { 0 | 1 < USF_TN2 : bit (3) > }
    { 0 | 1 < USF_TN3 : bit (3) > }
    { 0 | 1 < USF_TN4 : bit (3) > }
    { 0 | 1 < USF_TN5 : bit (3) > }
    { 0 | 1 < USF_TN6 : bit (3) > }
    { 0 | 1 < USF_TN7 : bit (3) > }
   | 1            -- Timeslot Allocation with Power Control Parameters
    < ALPHA >
    { 0 | 1 < USF_TN0 : bit (3) >
        < GAMMA_TN0 : bit (5) > }
    { 0 | 1 < USF_TN1 : bit (3) >
        < GAMMA_TN1 : bit (5) > }
    { 0 | 1 < USF_TN2 : bit (3) >
         < GAMMA_TN2 : bit (5) > }
    { 0 | 1 < USF_TN3 : bit (3) >
        < GAMMA_TN3 : bit (5) > }
    { 0 | 1 < USF_TN4 : bit (3) >
        < GAMMA_TN4 : bit (5) > }
    { 0 | 1 < USF_TN5 : bit (3) >
        < GAMMA_TN5 : bit (5) > }
    { 0 | 1 < USF_TN6 : bit (3) >
        < GAMMA_TN6 : bit (5) > }
    { 0 | 1 < USF_TN7 : bit (3) >
        < GAMMA_TN7 : bit (5) > } } ;

<Fixed Allocation struct PTR > ::=
  { 0 < UPLINK_TIMESLOT_ALLOCATION : bit (8) >
   | 1  < Power Control Parameters IE >  }
  < FINAL_ALLOCATION : bit (1) >
  < DOWNLINK_CONTROL_TIMESLOT: bit (3) >
  { 0 | 1 < P0 : bit (4) >
      < BTS_PWR_CTRL_MODE : bit (1) > 
      < PR_MODE : bit (1) > }
  { 0 | 1 < Measurement Mapping struct >  }
  < TBF Starting Time : < Starting Frame Number Description IE > >
  { 0 { 0     -- with length of Allocation Bitmap
      < BLOCKS_OR_BLOCK_PERIODS : bit (1) >
      < ALLOCATION_BITMAP_LENGTH : bit (7) >
      < ALLOCATION_BITMAP : bit (val(ALLOCATION_BITMAP_LENGTH)) >
     | 1      -- without length of Allocation Bitmap (fills remainder of the message)
      < ALLOCATION_BITMAP : bit ** > }
     ! < Message escape : 1 bit (*)  > };


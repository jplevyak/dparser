< Packet PSI Status message MO > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 000111 >
< Packet PSI Status message content > M V b:178
   
  ; 


< Packet PSI Status message content > ::=  
  < Global TFI IE > 
  < PBCCH_CHANGE_MARK : bit (3) >
  < PSI Message List struct > 
  < Unknown PSI Message List struct > 
  < padding bits > ;

< PSI Message List struct > ::=
  { 0 | 1 < MESSAGE_TYPE : bit (6) >
          < PSIX_CHANGE_MARK : bit (2) >
          { 0 | 1 < PSIX_COUNT : bit (4) >
          < Instance bitmap : bit (val(PSIX_COUNT) + 1) > } 
  } ** 0
  < ADDITIONAL_MSG_TYPE : bit >; 

< Unknown PSI Message List struct > ::=
  { 0 | 1 < MESSAGE_TYPE : bit (6) > } ** 0
  < ADDITIONAL_MSG_TYPE : bit > ;

< PSI1 message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 110001 >
< PSI1 message content >  M V b:178-?
; 



< PSI1 message content > ::=  
  < PAGE_MODE >
  < PBCCH_CHANGE_MARK : bit (3) >
  < PSI_CHANGE_FIELD : 
            < Update of unspecified PSI message/s:  0000 > |
            < Unknown:                              0001 > |
            < PSI2 updated:                         0010 > |
            < PSI3/PSI3bis updated:                 0011 > |
            < PSI4 updated:                         0100 > |
            < PSI5 updated:                         0101 > |
            < PSI6 updated:                         0110 > |
            < PSI7 updated:                         0111 > |
            < PSI8 updated:                         1000 > |
            < Update of unknown SI message type: bitrange(1001 to 1111)> 
  >
  < PSI1_REPEAT_PERIOD : bit (4) >
  < PSI_COUNT_LR : bit (6) >
  { 0 | 1 < PSI_COUNT_HR : bit (4) > }
  < MEASUREMENT_ORDER: bit (1) >
  < GPRS Cell Options IE > 
  < PRACH Control Parameters IE > 
  < PCCCH Organization Parameters IE > 
  < Global Power Control Parameters IE > 
  < PSI_STATUS_IND : bit >
  {  
       null 
     | < Receiver compatible with earlier release : 0 > 
     | < Additions in R99 : 1 > 
       < MSCR : bit >
       < SGSNR : bit >
       < BAND_INDICATOR : bit >
       -- qcswilk < padding bits > 
  }
  < spare bits > -- qcswilk 030415 
   ! < Distribution part error : bit (*)  > ;

< PSI2 message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 110010 >
< PSI2 message content >  M V b:178-?
; 

< PSI2 message content > ::=  
  < PAGE_MODE >
  < PSI2_CHANGE_MARK : bit (2) >
  < PSI2_INDEX : bit (3) >
  < PSI2_COUNT : bit (3) >
  { { 0 | 1 < Cell Identification IE > }
    { 0 | 1 < Non GPRS Cell Options IE > }
    < Reference Frequency Lists struct > 
    < Cell Allocation Lists struct > 
    < GPRS Mobile Allocations Lists struct > 
    < PCCCH Description Lists struct > 
		    { 
       null 
     | < Receiver compatible with earlier release : 0 > 
     | < Additions in R99 : 1 > 
        { 0 | 1 < COMPACT Control Info struct > }
        { 0 | 1 < Additional PSI Messages struct > }
        < spare bits > 
	  } 
	} // -- truncation at end of message allowed, bits '0' assumed
   < spare bits > -- qcswilk 030415 	
   ! < Distribution part error : bit (*)  > ;

< Reference Frequency Lists struct > ::= { 0 | 1 < Reference Frequency struct > } ** 0;

-- < Reference Frequency struct R >::=
--  0 | { 1 < Reference Frequency struct > < Reference Frequency struct R > };

< Reference Frequency struct >::=
  < RFL_NUMBER : bit (4) >
  < Length of RFL contents : bit (4) >
  < RFL contents : octet (val(Length of RFL contents) + 3) > ;
   
< Cell Allocation Lists struct > ::= { 0 | 1 < Cell Allocation struct > } ** 0 ;

< Cell Allocation struct > ::=
  < RFL_NUMBER : bit (4) > ;

< GPRS Mobile Allocations Lists struct > ::= 
{ 0 | 1 < GPRS Mobile Allocations struct > } ** 0 
; 

< GPRS Mobile Allocations struct > ::=
  < MA_NUMBER : bit (4) >
  < GPRS Mobile Allocation IE > ;

< PCCCH Description Lists struct > ::= 
{ 0 | 1 < PCCCH Description struct > } ** 0 
; 


< PCCCH Description struct > ::=
  < TSC : bit (3) >
  { 0 < Non-Hopping PCCCH Carriers Lists struct > 
	  | 1 < MA_NUMBER : bit (4) >
    < Hopping PCCCH Carriers Lists struct >  } ;

< Non-hopping PCCCH Carriers Lists struct > ::= 
{ 0 | 1 < Non-Hopping PCCCH Carriers struct > } ** 0 ;

<Non-Hopping PCCCH Carriers struct  >::=
  < ARFCN : bit (10) >
  < TIMESLOT_ALLOCATION : bit (8) > ;

< Hopping PCCCH Carriers Lists struct > ::= { 0 | 1 < Hopping PCCCH Carriers struct > } ** 0 ;

< Hopping PCCCH Carriers struct > ::=
  < MAIO : bit (6) >
  < TIMESLOT_ALLOCATION : bit (8) > ;

< COMPACT Control Info struct > ::=
  < Large Cell Operation : bit (1) >
  {0 | 1 < Number of Idle Blocks struct> }
  {0 | 1 <N_CCCH_NH : bit (4) >} ;

<Number of Idle Blocks struct> ::=
{0 | 1 { < NIB_CCCH_0 : bit (4) > } }
{0 | 1 { < NIB_CCCH_1 : bit (4) > } }
{0 | 1 { < NIB_CCCH_2 : bit (4) > } }
{0 | 1 { < NIB_CCCH_3 : bit (4) > } } ;

< Additional PSI Messages struct > ::=
  < NON_GSM_INFORMATION : bit(2) > 
  < PSI8_BROADCAST : bit (1) >
  < PSI3ter_BROADCAST : bit (1) > 
  < PSI3quater_BROADCAST : bit (1) > ;

< PSI3 message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 110011 >
< PSI3 message content >  M V b:178-?
   
  ; 


< PSI3 message content > ::=  
  < PAGE_MODE >
  < PSI3_CHANGE_MARK : bit (2) >
  < PSI3_BIS_COUNT : bit (4) >
  < Serving Cell params struct > 
  < Gen Cell Sel struct > 
  < Neighbour Cell parameters : { 0 | 1 < Neighbour Cell params struct >  } ** 0 >
  {  
       null 
     | < Receiver compatible with earlier release : 0 > 
     | < Additions in R98 : 1 > 
        < LSA ID information struct > 
        { 0 | 1 < LSA Parameters :< LSA Parameters IE >> }
        {  
             null 
           | < Receiver compatible with earlier release : 0 > 
           | < Additions in R99 : 1 > 
             {00}   -- The values '01', '10' and '11' were allocated in an earlier version of the protocol
                    -- and shall not be used.
             { 0 | 1 < COMPACT Information struct > }
             0  -- The value '1' was used in an earlier version of the protocol and shall not be used.
        }
  }
  < padding bits > 
  ! < Distribution part error : bit (*)  > ;

< Serving Cell params struct > ::=
  < CELL_BAR_ACCESS_2 : bit >
  < EXC_ACC : bit>
  < GPRS_RXLEV_ACCESS_MIN : bit (6) >
  < GPRS_MS_TXPWR_MAX_CCH : bit (5) >
  { 0 | 1 < HCS struct >  } 
  < MULTIBAND_REPORTING : bit (2) >;

< Gen Cell Sel struct > ::=
  < GPRS_CELL_RESELECT_HYSTERESIS : 
                                    < 0 dB:   000 > |
                                    < 2 dB:   001 > |
                                    < 4 dB:   010 > |
                                    < 6 dB:   011 > |
                                    < 8 dB:   100 > |
                                    < 10 dB:  101 > |
                                    < 12 dB:  110 > |
                                    < 14 dB:  111 > 
  >
  < C31_HYST : bit (1) >
  < C32_QUAL: bit (1) >
  < RANDOM_ACCESS_RETRY : bit (1) >
  { 0 | 1 < T_RESEL : 
                      < 5 seconds:    000 > |
                      < 10 seconds:   001 > |
                      < 15 seconds:   010 > |
                      < 20 seconds:   011 > |
                      < 30 seconds:   100 > |
                      < 60 seconds:   101 > |
                      < 120 seconds:  110 > |
                      < 300 seconds:  111 > 
   > }
  { 0 | 1 < RA_RESELECT_HYSTERESIS : 
                                    < 0 dB:   000 > |
                                    < 2 dB:   001 > |
                                    < 4 dB:   010 > |
                                    < 6 dB:   011 > |
                                    < 8 dB:   100 > |
                                    < 10 dB:  101 > |
                                    < 12 dB:  110 > |
                                    < 14 dB:  111 > 
  > } ;

< Neighbour Cell params struct > ::=
  < START_FREQUENCY : bit (10) >
  < Cell Selection struct PSI3 >
	< NR_OF_REMAINING_CELLS : bit (4) >
  < FREQ_DIFF_LENGTH : bit (3) >
  { < FREQUENCY_DIFF : bit (1 + val(FREQ_DIFF_LENGTH)) >
    < Cell Selection struct PSI3 > } * (val(NR_OF_REMAINING_CELLS)) ;

< Cell Selection struct PSI3 > ::=
  < BSIC : bit (6) >
  < CELL_BAR_ACCESS_2 : bit >
  < EXC_ACC : bit >
  < SAME_RA_AS_SERVING_CELL : bit (1) >
  { 0 | 1 < GPRS_RXLEV_ACCESS_MIN : bit (6) >
    < GPRS_MS_TXPWR_MAX_CCH : bit (5) > }
  { 0 | 1 < GPRS_TEMPORARY_OFFSET : 
                                    < 0  dB:    000 > |
                                    < 10 dB:    001 > |
                                    < 20 dB:    010 > |
                                    < 30 dB:    011 > |
                                    < 40 dB:    100 > |
                                    < 50 dB:    101 > |
                                    < 60 dB:    110 > |
                                    < infinity: 111 > 
          >
          < GPRS_PENALTY_TIME : bit (5) > 
  }
  { 0 | 1 < GPRS_RESELECT_OFFSET : bit (5) > }
  { 0 | 1 < HCS struct > }
  { 0 | 1 < SI13 PBCCH Location struct > } ;

< SI13 PBCCH Location struct > ::=
  { 0 < SI13_LOCATION : bit (1) >
   | 1  < PBCCH_LOCATION : bit (2) >
    < PSI1_REPEAT_PERIOD : bit (4) > } ;

< LSA ID information struct > ::=
  { 0 | 1 { 0 < LSA_ID : bit (24) >
          | 1 < ShortLSA_ID : bit (10) > } 
  } ** 0 ;

< COMPACT Information struct > ::=
  < Cell identification IE >
  { 0 | 1 < COMPACT Neighbour Cell params struct > } ** 0 ;

< COMPACT Neighbour Cell params struct > ::=
  < START_FREQUENCY : bit (10) >
  < COMPACT Cell Selection struct >
  < NR_OF_REMAINING_CELLS : bit (4) >
  < FREQ_DIFF_LENGTH : bit (3) >
  { < FREQUENCY_DIFF : bit (1 + val(FREQ_DIFF_LENGTH)) >
    < COMPACT Cell Selection struct > } 
                                    * (val(NR_OF_REMAINING_CELLS));

< COMPACT Cell Selection struct > ::=
  < BSIC : bit (6) >
  < CELL_BAR_ACCESS_2 : bit >
  < EXC_ACC : bit >
  < SAME_RA_AS_SERVING_CELL : bit (1) >
  { 0 | 1 < GPRS_RXLEV_ACCESS_MIN : bit (6) >
      < GPRS_MS_TXPWR_MAX_CCH : bit (5) > }
  { 0 | 1 < GPRS_TEMPORARY_OFFSET : bit (3) >
      < GPRS_PENALTY_TIME : bit (5)> }
  { 0 | 1 < GPRS_RESELECT_OFFSET : bit (5) > }
  { 0 | 1 < HCS struct > }
  { 0 | 1 < TIME_GROUP : bit (2) > }
  { 0 | 1 < GUAR_CONSTANT_PWR_BLKS : bit (2) >} ;

< PSI3 bis message MT > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 110100 >
< PSI3 bis message content >  M V b:178-?
   
  ; 


< PSI3 bis message content > ::=  
  < PAGE_MODE >
  < PSI3_CHANGE_MARK : bit (2) >
  < PSI3_BIS_INDEX : bit (4) >
  < PSI3_BIS_COUNT : bit (4) >
  { < Neighbour cell parameters : { 0 | 1 < Neighbour cell params struct > } ** 0 >
    < Neighbour Cell parameters 2 : { 0 | 1 < Neighbour Cell params 2 struct > } ** 0 >

  { 
       null 
     | < Receiver compatible with earlier release : 0 > 
     | < Additions in R98 : 1 > 
    { 0 | 1 < LSA Parameters IE > }

  { 
       null 
     | < Receiver compatible with earlier release : 0 > < SPARE BITS > 
     | < Additions in R99 : 1 > 
      < COMPACT Neighbour Cell Parameters : { 0 | 1 < COMPACT Neighbour Cell params 2 struct > } ** 0 >
      0 -- The value '1' was used in an earlier version of the protocol and shall not be used.
    
  < padding bits > } } } //   -- truncation at end of message allowed, bits '0' assumed
   < spare bits > -- qcswilk 030415 
   ! < Distribution part error : bit (*)  > ;

< Neighbour Cell params 2 struct > ::=
  { 00        -- Message escape
    { 0 | 1 
      < NCP2 Repeat struct >
          < CELL_PARAMS_POINTER : bit (2) > } ** 0       --Up to four pointers to the 'Neigbour parameter set
    < Neighbour parameter set struct > * (1 + max(CELL_PARAMS_POINTER))

   !  < Message escape: { 01 | 10 | 11 } bit** > } ;   -- Reserved for future use


< NCP2 Repeat struct > ::=

  { 1   < START_FREQUENCY : bit (10) >      -- Multiple START FREQ/FREQ DIFF sets may be defined
    < NCP2 Property struct >
    {
     < NR_OF_REMAINING_CELLS : bitrange( 0001 to 1111 )  >
        < FREQ_DIFF_LENGTH : bit (3) >
        { < FREQUENCY_DIFF : bit (1 + val(FREQ_DIFF_LENGTH)) >
        < NCP2 Property struct > } * (val(NR_OF_REMAINING_CELLS))
        < NCP2 Repeat struct >  -- "**Recursively repeated"
     |  
      0000 
    }                           -- Break recursion (NR_OF_REMAINING_CELLS == 0)
   | 0 } ;                      -- End recursion (no more START_FREQUENCY)

< NCP2 Property struct > ::=
    < SAME_RA_AS_SERVING_CELL : bit (1) >
    < CELL_BAR_ACCESS_2 : bit >
  < BCC : bit (3) > ;

< Neighbour parameter set struct > ::=
  { 0 | 1 < NCC : bit (3) > }
  < EXC_ACC : bit >
  { 0 | 1 < GPRS_RXLEV_ACCESS_MIN : bit (6) > }
  { 0 | 1 < GPRS_MS_TXPWR_MAX_CCH : bit (5) > }
  { 0 | 1 < PRIORITY_CLASS : bit (3) > }
  { 0 | 1 < HCS_THR : bit (5) >}
  { 0 | 1 < SI13_PBCCH_LOCATION struct >  }
  < GPRS_TEMPORARY_OFFSET : bit (3) >
  < GPRS_PENALTY_TIME : bit (5) >
  < GPRS_RESELECT_OFFSET : bit (5) > ;

< COMPACT Neighbour Cell params 2 struct > ::=
  { 00        -- Message escape
    { 0 | 1 < COMPACT NCP2 Repeat struct >
            < CELL_PARAMS_POINTER : bit (2) > } ** 0    -- Up to four pointers to the 'C Neighbour parameter set' 
    <COMPACT Neighbour parameter set : 
              <COMPACT Neighbour parameter set struct > > * (1+ max(CELL_PARAMS_POINTER))

   !  < Message escape: { 01 | 10 | 11 } bit** > } ;   -- Reserved for future use

< COMPACT NCP2 Repeat struct > ::=

  { 1   < START_FREQUENCY : bit (10) >      -- Multiple START FREQ/FREQ DIFF sets may be defined
    < COMPACT NCP2 Property struct >
    {   < NR_OF_REMAINING_CELLS : {bitrange( 0001 to 1111 )} >
      < FREQ_DIFF_LENGTH : bit (3) >
      { < FREQUENCY_DIFF : bit (1 + val(FREQ_DIFF_LENGTH)) >
        < COMPACT NCP2 Property struct > } * (val(NR_OF_REMAINING_CELLS))

      < COMPACT NCP2 Repeat struct >    -- "**Recursively repeated"
     |  0000 }                          -- Break recursion (NR_OF_REMAINING_CELLS == 0)
    | 0 } ;                               -- End recursion (no more START_FREQUENCY)

< COMPACT NCP2 Property struct > ::=
  < SAME_RA_AS_SERVING_CELL : bit (1) >
  < CELL_BAR_ACCESS_2 : bit >
  < BCC : bit (3) > 
  { 0 | 1 < TIME_GROUP : bit (2) > };

< COMPACT Neighbour parameter set struct > ::=
  { 0 | 1 < NCC : bit (3) > }
  < EXC_ACC : bit >
  { 0 | 1 < GPRS_RXLEV_ACCESS_MIN : bit (6) > }
  { 0 | 1 < GPRS_MS_TXPWR_MAX_CCH : bit (5) > }
  { 0 | 1 < GPRS_PRIORITY_CLASS : bit (3) > }
  { 0 | 1 < GPRS_HCS_THR : bit (5) >}
  < GPRS_TEMPORARY_OFFSET : bit (3) >
  < GPRS_PENALTY_TIME : bit (5) >
  < GPRS_RESELECT_OFFSET : bit (5) >
  { 0 | 1 < GUAR_CONSTANT_PWR_BLKS : bit (2) >} ;


  { { 0 | 1 < Real Time Difference Description struct > }
    { 0 | 1 < GPRS REP PRIORITY Description struct > }
    < padding bits > } //   -- truncation at end of message allowed, bits '0' assumed
   < spare bits > -- qcswilk 030415 
   ! < Distribution part error : bit (*)  > ;

< Real Time Difference Description struct > ::=
  { 0 | 1 { 0 | 1 < Cell_Index_Start_RTD : bit (7) > }    --default value=0
      < RTD6 Struct >
      { 1 | 0 < RTD6 Struct > } **1 }   -- '0' : increment by 1 the index of the GSM Neighbour Cell list

  { 0 | 1 { 0 | 1 < Cell_Index_Start_RTD : bit (7) > }    --default value=0
      < RTD12 Struct >
      { 1 | 0 < RTD12 Struct > } **1 };   -- '0' : increment by 1 the index of the GSM Neighbour Cell list

< RTD6 Struct > ::=
  { 0 | 1 < RTD : bit (6) > } ;                 --'0' means no RTD for this cell


    { 0   -- Message escape
      { < CHANNEL_CODING_COMMAND : 
                                  < CS-1: 00 > |
                                  < CS-2: 01 > |
                                  < CS-3: 10 > |
                                  < CS-4: 11 >
         >
        < TLLI_BLOCK_CHANNEL_CODING : bit (1) >
        < Packet Timing Advance IE > 
        { 0 | 1 < Frequency Parameters IE > }
        {  01  < Dynamic Allocation struct PUA > 
         | 10 <  Single Block Allocation struct > 
         | 00 < extension >
         | 11 < Fixed Allocation struct PUA >  }
           { 
               null 
             | < Receiver compatible with earlier release : 0 >
             | < Additions in R99 : 1 > 
                { 0 | 1 <Packet Extended Timing Advance : bit (2)> }
                < padding bits > 
           }
         ! < Non-distribution part error : bit (*)  > } 
    | 1  -- Message escape bit used to define EGPRS message contents  -MESSAGE
      { 00 {  { 0 | 1 <CONTENTION_RESOLUTION_TLLI : bit(32) > }
        { 0 | 1 < COMPACT reduced MA IE > }
        < EGPRS Modulation and Coding IE >
          < RESEGMENT : bit (1) >
          < EGPRS Window Size IE >
          { 0 | 1 < Access Technologies Request : Access Technologies Request struct >}
          < ARAC RETRANSMISSION REQUEST : bit (1) >
          < TLLI_BLOCK_CHANNEL_CODING : bit (1) >
            { 0 | 1 < BEP_PERIOD2 : bit(4) > }
        < Packet Timing Advance IE > 
            { 0 | 1 <Packet Extended Timing Advance : bit (2)> }
           { 0 | 1  <  Frequency Parameters IE >  }
           { 01 < Dynamic Allocation struct PUA > 
           | 10 < Multi Block Allocation struct > 
         | 00 < extension >
         | 11 < Fixed Allocation struct PUA > }
         < padding bits > 
         ! < Non-distribution part error : bit (*)  > } 
     ! < Message escape : { 01| 10 | 11 } bit (*)  > } } -- Extended for future changes
     ! < Address information part error : bit (*)  > } 
   < spare bits > -- qcswilk 030415 
   ! < Distribution part error : bit (*)  > ;

<extension> ::= -- Future extension can be done by modifying this structure
  null ;

<Dynamic Allocation struct PUA > ::= 
  < EXTENDED_DYNAMIC_ALLOCATION : bit (1) >
  { 0 | 1 < P0 : bit (4) > 
  < PR_MODE : bit (1) > }
  < USF_GRANULARITY : bit (1) >
  { 0 | 1 < UPLINK_TFI_ASSIGNMENT : bit (5) > }
  { 0 | 1 < RLC_DATA_BLOCKS_GRANTED : bit (8) > }
  { 0 | 1 < TBF Starting Time : < Starting Frame Number Description IE > > }
  { 0           -- Timeslot Allocation
    { 0 | 1 < USF_TN0 : bit (3) > }
    { 0 | 1 < USF_TN1 : bit (3) > }
    { 0 | 1 < USF_TN2 : bit (3) > }
    { 0 | 1 < USF_TN3 : bit (3) > }
    { 0 | 1 < USF_TN4 : bit (3) > }
    { 0 | 1 < USF_TN5 : bit (3) > }
    { 0 | 1 < USF_TN6 : bit (3) > }
    { 0 | 1 < USF_TN7 : bit (3) > }
  | 1           -- Timeslot Allocation with Power Control Parameters
    < ALPHA >
    { 0 | 1 < USF_TN0 : bit (3) >
        < GAMMA_TN0 : bit (5) > }
    { 0 | 1 < USF_TN1 : bit (3) >
        < GAMMA_TN1 : bit (5) > }
    { 0 | 1 < USF_TN2 : bit (3) >
        < GAMMA_TN2 : bit (5) > }
    { 0 | 1 < USF_TN3 : bit (3) >
        < GAMMA_TN3 : bit (5) > }
    { 0 | 1 < USF_TN4 : bit (3) >
        < GAMMA_TN4 : bit (5) > }
    { 0 | 1 < USF_TN5 : bit (3) >
        < GAMMA_TN5 : bit (5) > }
    { 0 | 1 < USF_TN6 : bit (3) >
        < GAMMA_TN6 : bit (5) > }
    { 0 | 1 < USF_TN7 : bit (3) >
        < GAMMA_TN7 : bit (5) > } } ;

<Single Block Allocation struct > ::=
  < TIMESLOT_NUMBER : bit (3) > 
  { 0 | 1 < ALPHA >
  < GAMMA_TN : bit (5) >}
  { 0 | 1 < P0 : bit (4) >
      < BTS_PWR_CTRL_MODE : bit (1) > 
      < PR_MODE  : bit (1) > }
  < TBF Starting Time : < Starting Frame Number Description IE > > ;

<Fixed Allocation struct PUA > ::= 
  { 0 | 1 < UPLINK_TFI_ASSIGNMENT : bit (5) > }
  < FINAL_ALLOCATION : bit (1) >
  < DOWNLINK_CONTROL_TIMESLOT: bit (3) >
  { 0 | 1 < P0 : bit (4) >
      < BTS_PWR_CTRL_MODE : bit (1) > 
      < PR_MODE : bit (1) > }
  { 0 < TIMESLOT_ALLOCATION : bit (8) >
   | 1  < Power Control Parameters IE >  }
  < HALF_DUPLEX_MODE : bit (1) >
  < TBF Starting Time : < Starting Frame Number Description IE > > 
  { 0 { 0     -- with length of Allocation Bitmap
      < BLOCKS_OR_BLOCK_PERIODS : bit (1) >
      < ALLOCATION_BITMAP_LENGTH : bit (7) > 
      < ALLOCATION_BITMAP : bit (val(ALLOCATION_BITMAP_LENGTH)) >
     | 1      -- without length of Allocation Bitmap (fills remainder of the message)
      < ALLOCATION_BITMAP : bit ** > } 
     ! < Message escape : 1 bit (*)  > } ;

< Multi Block Allocation struct > ::=
  < TIMESLOT_NUMBER : bit (3) >
  { 0 | 1 < ALPHA >
  < GAMMA_TN : bit (5) >}
  { 0 | 1 < P0 : bit (4) >
      < BTS_PWR_CTRL_MODE : bit (1) >
      < PR_MODE : bit (1) > }
  < TBF Starting Time : < Starting Frame Number Description IE > >
  < NUMBER OF RADIO BLOCKS ALLOCATED: bit (2)>;

<Access Technologies Request struct> ::= -- recursive structure allows any combination of Access technologies
  <Access Technology Type : bit (4)>
  { 0 | 1 <Access Technology Type : bit (4)> }**0;

< Packet pause message MO > ::=  -MESSAGE
< MESSAGE_TYPE : bit(6) == 001001 >
< Packet pause message content > M V b:178
   
  ; 


